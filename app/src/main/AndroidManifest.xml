<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jkkniugmail.rubel.coursedetails">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".CourseAddLayoutActivity" />
        <activity android:name=".StudentEditLayoutActivity" />
        <activity android:name=".CourseDetailActivity" />
        <activity android:name=".CourseListView" />
        <activity android:name=".StudentDetailActivity" />
    </application>

</manifest>